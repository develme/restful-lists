{
    "name": "develme/restful-lists",
    "description": "Adds filter, order, and pagination to your restful resource lists",
    "type": "library",
    "license": "MIT License",
    "authors": [
        {
            "name": "Verron Knowles",
            "email": "Verron.Knowles@develme.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^8.0",
        "nesbot/carbon": "^2.45"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-watcher": "^1.23",
        "fakerphp/faker": "^1.13",
        "vlucas/phpdotenv": "^5.3",
        "symfony/var-dumper": "^5.2",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "DevelMe\\RestfulList\\": "src/"
        },
        "src/": "Develme\\RestfulList\\"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Database\\Factories\\": "tests/database/factories/",
            "Tests\\Database\\Seeders\\": "tests/database/seeders/"
        }
    }
}
